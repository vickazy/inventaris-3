<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_transaksi"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="id.sch.smktelkom_mlg.project.xiirpl502122232.inventarissekolah.Transaksi">

    <Button
        android:id="@+id/buttonpengadaanbarang"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Pengadaan Barang" />

    <Button
        android:id="@+id/buttonpengkodean"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="50dp"
        android:text="Pengkodean" />

    <Button
        android:id="@+id/buttonpenempatanbarang"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="100dp"
        android:text="Penempatan Barang" />

    <Button
        android:id="@+id/buttonmutasibarang"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="150dp"
        android:text="Mutasi Barang" />

    <Button
        android:id="@+id/buttonpemeliharaanbarang"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="200dp"
        android:text="Pemeliharaan Barang" />

</RelativeLayout>
